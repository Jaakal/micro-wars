{
  "name": "micro-wars",
  "version": "1.0.0",
  "description": "Space shooter game created using Phaser",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "heroku-postbuild": "webpack -p",
    "test": "jest",
    "dev": "webpack --mode development",
    "build": "webpack --mode production"
  },
  "keywords": [],
  "author": "Jaak Kivinukk",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/test/style-mock.js",
      "\\.(gif|ttf|eot|svg|png|wav|mp3|html)$": "<rootDir>/test/file-mock.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "autoprefixer": "^9.7.5",
    "babel-jest": "^25.4.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "file-loader": "^6.0.0",
    "jest": "^25.4.0",
    "jest-canvas-mock": "^2.2.0",
    "jquery": "^3.5.0",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-loader": "^3.0.0",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "phaser": "^3.22.0",
    "regenerator-runtime": "^0.13.5"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jaakal/micro-wars.git"
  },
  "bugs": {
    "url": "https://github.com/Jaakal/micro-wars/issues"
  },
  "homepage": "https://github.com/Jaakal/micro-wars#readme"
}
